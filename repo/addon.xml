<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.husham.com" name="Husham.com Repo" version="1.0.1" provider-name="husham.com">
	<extension point="xbmc.addon.repository" name="Husham.com Repository">
		<info compressed="false">https://raw.githubusercontent.com/hmemar/husham.com/master/repo/addon.xml</info>
		<checksum>https://raw.githubusercontent.com/hmemar/husham.com/master/repo/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/hmemar/husham.com/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>The Husham.com Repo</summary>
		<description>Download and install addons from husham.com repo</description>
		<platform>all</platform>
	</extension>
</addon>


<addon id="plugin.video.RobinHood-PPL-List"
       name="RobinHood-People List"
       version="2.0.1"
       provider-name="Husham.com">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">RobinHood People Lists - downloaded for free from:
http://www.husham.com</summary>
    <description lang="en">RobinHood-People List - The shadow crew finest addon
	The ShadowCrew Has been working round the clock to make this addon working great for all.
	http://www.husham.com
	P.S. Special Thanks to Shani for his Excellent work for LiveStream Pro add-on
</description>
    <platform>all</platform>
  </extension>
</addon>



<addon
  id="plugin.video.haydargames"
  version="1.0.3"
  name="HaydarGames"
  provider-name="Husham.com">
