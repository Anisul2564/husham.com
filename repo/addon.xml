    <import addon="xbmc.python" version="2.1.0"/>
  	<import addon="script.module.buggalo" version="1.1.2"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Addon made by Husham Memar</summary>
    <description>Please enjoy and visit the Memar games channels to give Haydar a thank you for his selection of game channels to watch
	The Haydar Memar Games Youtube Channels: Addon made by Husham Memar http://www.husham.com:
	This Addon was a collection of all the Games chanels in youtube watched by kids these days:
	Haydar Memar wanted to share his gammes channels with everyone:
	Haydar Memar usually play games and post them in the memar games channels:
	P.S. This addon will only work with KODI Helix or above
		</description>
   
    <platform>all</platform>
  </extension>
</addon>


</addons>
